hostname: {{ harbor_hostname }}
http:
  port: 80
harbor_admin_password:  {{ harbor_admin_password }}
database:
  password: root123
  max_idle_conns: 100
  max_open_conns: 900

data_volume: /var/harbor
trivy:
  ignore_unfixed: false
  skip_update: false
  insecure: false

jobservice:
  max_job_workers: 10

notification:
  webhook_job_max_retry: 10

chart:
  absolute_url: disabled

log:
  level: info
  local:
    rotate_count: 50
    rotate_size: 200M
    location: /var/log/harbor

proxy:
  http_proxy:
  https_proxy:
  no_proxy:
  components:
    - core
    - jobservice
    - trivy

metric:
  enabled: false
  port: 9090
  path: /metrics
